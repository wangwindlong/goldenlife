<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.appbar.MaterialToolbar xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@id/base_toolbar"
    android:layout_width="match_parent"
    android:layout_height="?attr/actionBarSize"
    android:background="?attr/colorPrimary"
    android:theme="@style/base_ToolBarStyle"
    app:titleTextAppearance="@style/base_ToolbarTextAppearance"
    app:colorPrimaryDark="@color/base_black"
    app:contentInsetLeft="0dp"
    app:contentInsetStart="0dp"
    app:layout_collapseMode="pin"
    app:layout_collapseParallaxMultiplier="0.7"
    app:logo="@null"
    app:navigationIcon="@mipmap/base_ic_arrow_back"
    app:popupTheme="@style/base_AppTheme.PopupOverlay"
    app:titleCentered="true"
    app:titleMarginStart="0dp"
    app:titleMargins="0dp"
    app:titleTextColor="@color/base_black" />
    <!-- app:titleTextAppearance="@style/Theme.IOSched.Sessions"-->